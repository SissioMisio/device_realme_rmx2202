<resources>
    <!-- Set default & peak refresh rate  -->
    <integer name="config_defaultRefreshRate">120</integer>
    <integer name="config_defaultPeakRefreshRate">120</integer>
    
    <!-- Controls the navigation bar interaction mode:
         0: 3 button mode (back, home, overview buttons)
         1: 2 button mode (back, home buttons + swipe up for overview)
         2: gestures only for back, home and overview -->
    <integer name="config_navBarInteractionMode">2</integer>
    
    <!-- Whether device has warp charging support -->
    <bool name="config_hasWarpCharger">true</bool>

   <!-- Paths to fast charging status file to detect whether an oem fast charger is active -->
    <string name="config_oemFastChargerStatusPath" translatable="false">/sys/class/oplus_chg/battery/voocchg_ing</string>
   
   <!-- Whether devices suports in-display fingerprint when screen is off -->
    <bool name="config_supportsScreenOffUdfps">true</bool>
    
</resources>
